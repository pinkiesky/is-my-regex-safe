{
  "name": "is-my-regex-safe",
  "version": "0.0.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinkiesky/is-my-regex-safe.git"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-12-18 --proxy 5173 -- npm run dev",
    "pages:deploy": "npm run build && wrangler pages deploy"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.1",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/adapter-cloudflare": "^7.2.3",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4",
    "wrangler": "^4.33.1"
  },
  "type": "module",
  "dependencies": {
    "modern-normalize": "^2.0.0",
    "safe-regex2": "^3.1.0",
    "simpledotcss": "^2.2.1"
  }
}
